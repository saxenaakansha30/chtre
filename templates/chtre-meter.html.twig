{#
/**
 * @file
 * Default theme implementation for the meter render element.
 *
 * Available variables:
 * - attributes: Attributes for the meter tag.
 * - content: The text to display within the meter tag.
 *
 * @ingroup themeable
 * @ingroup chtre
 */
#}
<div>
  <label for={{ attributes.id }}>{{ label_content }}</label>
  <meter {{ attributes }}>{{ content }}</meter>
</div>
